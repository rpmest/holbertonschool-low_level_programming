#include <stdio.h>

/**
 * main - Prints the alphabet in lowercase and then in uppercase
 *
 * Return: 0
 */
int main(void)
{
	int low, up;
	for (low = 97; low <= 122; low++)
		putchar(low);
	for (up = 65; up <= 90; up++)
                putchar(up);
	/*Prints newline*/
	putchar(10);

	return (0);
}
